<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
</head>

<body>
    <div>
        <h1 class="text-danger d-flex  justify-content-center text-center m-5">Bienvenido!</h1>
        <div class="p-2 m-2 d-flex justify-content-center text-center">
            <ul>
                <% productsViews.forEach(productView=> { %>
                    <a class="text-decoration-none " href=<%=productView.linkReference%>>
                        <h2 class="mt-3">
                            <%= productView.title %>
                        </h2>
                    </a>
                    <% }) %>
            </ul>
        </div>
    </div>
</body>

</html>